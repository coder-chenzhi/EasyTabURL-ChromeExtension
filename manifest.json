{
   "manifest_version": 2,
   "browser_action": {
      "default_icon": "16.png",
      "default_popup": "popup.html"
   },
   "description": "Conveniently manage your opened tab",
   "icons": {
      "16": "16.png",
      "48": "48.png",
      "128": "128.png"
   },
   "name": "Tab Manager",
   "permissions": [
      "tabs"
   ],
   "version": "0.1"
}