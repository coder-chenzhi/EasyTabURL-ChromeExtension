{
   "name": "Easy Tab URL",
   "version": "0.3.1",
   "description": "Help you operate tabs' url more conveniently",
   "browser_action": {
      "default_icon": "16.png",
      "default_popup": "popup.html"
   },
   "icons": {
      "16": "16.png",
      "48": "48.png",
      "128": "128.png"
   },
   "permissions": [
      "tabs"
   ],
   "manifest_version": 2,
   "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}